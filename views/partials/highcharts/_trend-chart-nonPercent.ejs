$('#trend-chart').highcharts({
    chart: {
        type: 'spline'
    },
    title: {
        text: 'Wonder Trade Trends'
    },            
    xAxis: {
        type: 'datetime',
        dateTimeLabelFormats: { // don't display the dummy year
            day: '%b, %e'
        }
    },
    yAxis: {
        title: {
            text: 'Wonder Trades'
        },
        min: 0,
        labels: {
            format: '{value}'
        }
    },
    tooltip: {
        formatter: function() {
                return '<b>'+ this.series.name +':</b> '+ this.y;
        }
    },
    series: <%- trendingPokemonChart %>
});