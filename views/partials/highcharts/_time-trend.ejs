$('#time-trend-chart').highcharts({
    chart: {
        type: 'spline'
    },
    title: {
        text: 'WonderTrade Data By Time (CST)'
    },
    xAxis: {
        labels: {
            formatter: <% include __time-format.ejs %>
        }
    },
    yAxis: {
        title: {
            text: 'Percentage of total'
        },
        labels: {
            formatter: function(){return (this.value*100)+"%"}
        },
        min: 0
    },
    tooltip: {
        formatter: function() {
            return '<b>'+ (this.y*100).toFixed(2)+'%</b>';
        }
    },
    series: [<%- timeTrends %>]
});