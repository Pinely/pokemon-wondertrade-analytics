$('#date-trend-chart').highcharts({
    chart: {
        type: 'spline'
    },
    title: {
        text: 'Wonder Trade Trends Recorded'
    },            
    xAxis: {
        type: 'datetime',
        dateTimeLabelFormats: { // don't display the dummy year
            day: '%b, %e'
        },
        plotLines: <% include _eventPlotlines.ejs %>
    },
    yAxis: {
        title: {
            text: 'Wonder Trades (Daily Count)'
        },
        min: 0
    },
    tooltip: {
        formatter: function() {
                return '<b>'+ this.series.name +'</b><br/>'+ this.y;
        }
    },
    series: [<%- wondertradeTrends %>]
});