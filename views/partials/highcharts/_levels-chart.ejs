$('#levels-chart').highcharts({
    chart: {
        type: 'column',
        height: '300'
    },
    title: {
        text: 'Level Distribution'
    },            
    xAxis: {
        
    },
    yAxis: {
        min: 0,
        title: {
            text: 'Pokemon Count'
        }
    },
    tooltip: {
        formatter: function() {
                return '<b>'+ this.series.name +' '+this.x+'</b><br/>'+ this.y;
        }
    },
    plotOptions: {
        column: {
            pointPadding: 0.2,
            borderWidth: 0,
            showInLegend: false
        }
    },
    series: <%- levelBarChart %>
}); 